{
  "name": "dataloader-ui",
  "productName": "Bullhorn Data Loader",
  "version": "1.10.0",
  "description": "Desktop App for quickly importing CSV data into your Bullhorn CRM",
  "repository": "https://github.com/bullhorn/dataloader-ui",
  "author": "Bullhorn",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "ng build --aot --base-href . && npm run compile",
    "build:prod": "ng build --prod --base-href . && npm run compile",
    "compile": "tsc ./main-process/* --outDir dist",
    "download": "node download-latest-dataloader.js",
    "lint": "tslint --project tsconfig.json",
    "dist": "cd dist && electron-builder --publish always -mw -c.mac.identity=null",
    "package": "npm run download && npm run build:prod && npm run stage && npm run dist",
    "serve": "electron dist/main.js --serve",
    "stage": "copyfiles package*.json 'dataloader/**/*' dist && cd dist && npm install --prod && cd ..",
    "start": "npm run download && npm run build && npm run serve",
    "test": "ng test --code-coverage --watch false",
    "test:watch": "ng test",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "main": "main.js",
  "build": {
    "appId": "com.bullhorn.dataloader",
    "directories": {
      "buildResources": "../buildResources",
      "output": "../packages"
    },
    "asarUnpack": "dataloader/"
  },
  "dependencies": {
    "@angular/animations": "7.2.14",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.14",
    "@angular/compiler": "7.2.14",
    "@angular/core": "7.2.14",
    "@angular/forms": "7.2.14",
    "@angular/http": "7.2.14",
    "@angular/platform-browser": "7.2.14",
    "@angular/platform-browser-dynamic": "7.2.14",
    "@bullhorn/bullhorn-icons": "^2.6.0",
    "@bullhorn/bullhorn-types": "^1.9.0",
    "@bullhorn/dragula": "1.0.1",
    "ace-builds": "1.4.5",
    "angular2-text-mask": "9.0.0",
    "brace": "0.11.1",
    "chart.js": "2.8.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.1.4",
    "crypto-js": "^3.1.9-1",
    "date-fns": "1.30.1",
    "fast-csv": "^3.4.0",
    "glob": "^7.1.4",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "novo-elements": "^3.11.0",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.2",
    "text-mask-addons": "3.8.0",
    "universal-analytics": "^0.4.20",
    "uuidv4": "^4.0.0",
    "zone.js": "0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.8",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.14",
    "@angular/language-service": "7.2.14",
    "@types/chart.js": "^2.7.56",
    "@types/crypto-js": "^3.1.43",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "12.6.9",
    "codelyzer": "5.1.0",
    "copyfiles": "^2.1.1",
    "electron": "1.8.8",
    "electron-builder": "^20.11.1",
    "extract-zip": "^1.6.7",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "standard-version": "^7.0.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.2.4",
    "vrsource-tslint-rules": "^6.0.0"
  }
}
