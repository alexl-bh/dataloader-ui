{
  "name": "dataloader-ui",
  "productName": "DataLoader",
  "version": "1.0.0",
  "description": "Bullhorn DataLoader Desktop Application",
  "repository": "https://github.com/bullhorn/dataloader-ui",
  "author": "Nathan Dickerson",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "ng build --aot --base-href . && npm run compile",
    "build:prod": "ng build --prod --base-href .  && npm run compile",
    "compile": "tsc ./electron/* --outDir dist",
    "lint": "ng lint",
    "package": "npm run build:prod && npm run stage && electron-packager ./dist --overwrite",
    "serve": "electron dist/main.js",
    "stage": "npm run compile && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "start": "npm run build && npm run serve",
    "test": "ng test"
  },
  "main": "main.js",
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "core-js": "^2.5.1",
    "fast-csv": "^2.4.1",
    "novo-elements": "^2.1.20",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/language-service": "^4.4.4",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.0.32",
    "codelyzer": "~3.2.0",
    "copyfiles": "^1.2.0",
    "electron": "1.7.8",
    "electron-builder": "^19.34.1",
    "electron-packager": "^9.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.3",
    "vrsource-tslint-rules": "^5.1.1"
  }
}
